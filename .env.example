# Cloudflare Configuration
# Copy this file to .env and fill in your actual values

# Your Cloudflare Account ID (found in the right sidebar of your Cloudflare dashboard)
VITE_CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# Your KV Namespace ID (create a KV namespace first, then get the ID)
VITE_CLOUDFLARE_KV_NAMESPACE_ID=your_namespace_id_here

# Your Cloudflare API Token (create one with KV:Edit permissions)
VITE_CLOUDFLARE_API_TOKEN=your_api_token_here
